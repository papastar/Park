<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:zhy="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#f0f0f0"
    android:orientation="horizontal" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/selector_bt_item"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/lmo_item_lefttwo"
            android:layout_width="match_parent"
            android:layout_height="60dip"
            android:background="@drawable/selector_bt_item"
            android:gravity="center_vertical"
            android:paddingBottom="5dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="5dp" >

            <com.joyotime.qparking.view.RoundImageView
                android:id="@+id/lmo_iv_icontwo"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="5dp"
                android:scaleType="centerCrop"
                android:src="@drawable/lockadm_head"
                zhy:borderRadius="5dp"
                zhy:type="round" />

            <LinearLayout
                android:id="@+id/lmo_ll_toptwo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@id/lmo_iv_icon"
                android:layout_marginLeft="55dip"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/lmo_tv_locktitletwo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dip"
                    android:text="车锁名："
                    android:textColor="@color/black"
                    android:textSize="13sp" />

                <TextView
                    android:id="@+id/lmo_tv_locknametwo"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="60dip"
                    android:layout_weight="1"
                    android:text="000000"
                    android:textColor="@color/black"
                    android:textSize="13sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lmo_ll_bottomtwo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@id/lmo_iv_icontwo"
                android:layout_alignTop="@id/lmo_ll_toptwo"
                android:layout_marginLeft="50dip"
                android:layout_marginTop="20dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dip"
                    android:text="停车场："
                    android:textColor="@color/black"
                    android:textSize="13sp" />

                <TextView
                    android:id="@+id/lmo_tv_lockparkingtwo"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="60dip"
                    android:layout_weight="1"
                    android:text="000000"
                    android:textColor="@color/black"
                    android:textSize="13sp" />
            </LinearLayout>

            <com.joyotime.qparking.view.RoundImageView
                android:id="@+id/lmo_iv_iconcelltwo"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="55dp"
                android:scaleType="centerCrop"
                android:src="@drawable/gen_icon_phone"
                zhy:borderRadius="10dp"
                zhy:type="round" />

            <View
                android:layout_width="1dp"
                android:layout_height="25dip"
                android:layout_alignParentRight="true"
                android:layout_marginTop="10dip"
                android:layout_marginRight="45dp"
                android:background="@drawable/shape_line"
                />

            <ImageView
                android:id="@+id/lmo_iv_icondeletetwo"
                android:layout_width="45.0dip"
                android:layout_height="45.0dip"
                android:layout_alignParentRight="true"
                android:layout_marginRight="0dip"
                android:src="@drawable/gen_icon_position" />
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1.0px"
            android:layout_marginLeft="20dip"
            android:layout_marginRight="20dip"
            android:background="@drawable/shape_line" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="5dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="5dp" >

            <LinearLayout
                android:id="@+id/lmo_ll_bottmetwo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/lmo_tv_bottmelocktitletwo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车锁地址："
                    android:textColor="@color/gray"
                    android:textSize="13sp" />

                <TextView
                    android:id="@+id/lmo_tv_lockparkingaddresstwo"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="000000"
                    android:textColor="@color/gray"
                    android:textSize="13sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lmo_ll_bottmebottomtwo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车位编码："
                    android:textColor="@color/gray"
                    android:textSize="13sp" />

                <TextView
                    android:id="@+id/lmo_tv_lockusetimetwo"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="0000"
                    android:textColor="@color/gray"
                    android:textSize="13sp" />
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/lmo_item_righttwo"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="#00B2EE"
        android:onClick="true" >

        <TextView
            android:id="@+id/lmo_item_right_txttwo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:drawableLeft="@drawable/icon_nolink"
            android:gravity="center"
            android:text="导航"
            android:textColor="#000000"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/lmo_item_keyid_txttwo"
            android:layout_width="0dip"
            android:layout_height="0dip"
            android:text="" />

        <TextView
            android:id="@+id/lmo_item_mlong_txttwo"
            android:layout_width="0dip"
            android:layout_height="0dip"
            android:text="" />

        <TextView
            android:id="@+id/lmo_item_mlat_txttwo"
            android:layout_width="0dip"
            android:layout_height="0dip"
            android:text="" />
    </RelativeLayout>

</LinearLayout>